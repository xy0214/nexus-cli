{
  "hosting": {
    "public": "public",
    "rewrites": [
      {
        "source": "/version.json",
        "destination": "/version.json"
      },
      {
        "source": "**",
        "destination": "/install.sh"
      }
    ],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  }
}
